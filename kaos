end 
end)

SectionFarm:CreateToggle("Auto get Cash", {Toggled=false , Description = "Tps to all the Money that has been dropped/spawned"}, function(bool)
    getgenv().CashGrab = bool

    game:GetService('RunService').Stepped:connect(function()
    if getgenv().CashGrab then
    game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(11)
    end end)
    while getgenv().CashGrab == true do
        wait()
        pcall(
            function()
                for i, v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do
                    if v.ClassName == "Part" and v:FindFirstChild("ClickDetector") then
                        wait(1)
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                        wait(1)
                        fireclickdetector(v.ClickDetector)
                        wait(1)
                    end
                end
            end
        )
    end
    end)


    SectionFarm:CreateToggle("Auto Hospital Job", {Toggled=false , Description = "Auto completes Hospital Job for Money"}, function(bool)
